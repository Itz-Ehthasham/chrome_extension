{
  "manifest_version": 3,
  "name": "RickRoll YouTube",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["rick-astley.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
